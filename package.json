{
	"name": "@treeviz/gramps-sdk",
	"version": "1.0.0",
	"description": "TypeScript SDK for Gramps Web API - JWT authentication, REST API client, and data conversion",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./auth": {
			"types": "./dist/auth/index.d.ts",
			"import": "./dist/auth/index.js",
			"require": "./dist/auth/index.cjs"
		},
		"./api": {
			"types": "./dist/api/index.d.ts",
			"import": "./dist/api/index.js",
			"require": "./dist/api/index.cjs"
		},
		"./converters": {
			"types": "./dist/converters/index.d.ts",
			"import": "./dist/converters/index.js",
			"require": "./dist/converters/index.cjs"
		},
		"./types": {
			"types": "./dist/types/index.d.ts",
			"import": "./dist/types/index.js",
			"require": "./dist/types/index.cjs"
		},
		"./utils": {
			"types": "./dist/utils/index.d.ts",
			"import": "./dist/utils/index.js",
			"require": "./dist/utils/index.cjs"
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"test": "vitest run",
		"test:watch": "vitest",
		"lint": "tsc --noEmit",
		"clean": "rm -rf dist",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"gramps",
		"genealogy",
		"api",
		"sdk",
		"jwt",
		"typescript",
		"treeviz"
	],
	"author": "idavidka and @treeviz contributors",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^25.0.9",
		"jsdom": "^27.4.0",
		"tsup": "^8.0.0",
		"typescript": "^5.1.6",
		"vitest": "^3.0.7"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
